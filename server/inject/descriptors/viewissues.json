            "update": {
              "type": "none"
            }
          },
          "severity": {
            "type": "string",
            "update": {
              "type": "none"
            }
          },
          "assignedto": {
            "type": "string",
            "update": {
              "type": "none"
            }
          },
          "updated": {
            "type": "string",
            "update": {
              "type": "none"
            }
          }
        }
      },
      "update": {
        "type": "none"
      }
